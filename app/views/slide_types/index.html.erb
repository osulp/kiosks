<p id="notice"><%= notice %></p>

<h1>Slide Types</h1>

<div class="panel panel-primary">
  <div class="panel-heading">
    <h2 class="panel-title">All Slide Types</h2>
  </div>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @slide_types.each do |slide_type| %>
        <tr>
          <td><%= slide_type.name %></td>
          <td><%= link_to 'Show', slide_type %></td>
          <td><%= link_to 'Edit', edit_slide_type_path(slide_type) %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div> 

<br>

<%= link_to 'New Slide Type', new_slide_type_path, class: "btn btn-primary" %>
<%= link_to 'Admin Panel', rails_admin_path, class: "btn btn-default" %>
